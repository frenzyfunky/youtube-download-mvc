{
  "compilerOptions": {
    "jsx": "react",
    "module": "CommonJS",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "lib": [
      "ES2015",
      "es5",
      "dom"
    ],
    "outDir": "dist"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true
}
